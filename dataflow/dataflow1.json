{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SQLServerSink",
						"type": "DatasetReference"
					},
					"name": "CSVSourceDataset"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CSVSourceDataset",
				"CSVSourceDataset derive(column1 = ERROR_FUNCTION(''),",
				"          each(match())) ~> derivedColumn1"
			]
		}
	}
}