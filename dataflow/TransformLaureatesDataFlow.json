{
	"name": "TransformLaureatesDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TempJSONStore",
						"type": "DatasetReference"
					},
					"name": "TempJsonStoreSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ColumnMapping",
					"description": "Creating/updating the columns "
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> TempJsonStoreSource",
				"TempJsonStoreSource derive(column1 = ERROR_FUNCTION('')) ~> ColumnMapping"
			]
		}
	}
}